<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
"http://castor.org/mapping.dtd">
<mapping>

	<!-- Join -->
	<class name="ee.adit.pojo.JoinRequest">
		<map-to xml="Join" ns-uri="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid" />
		<field name="userType" type="java.lang.String">
			<bind-xml name="userType" location="keha" />
		</field>
		<field name="userName" type="java.lang.String">
			<bind-xml name="userName" location="keha" />
		</field>
	</class>

	<class name="ee.adit.pojo.JoinResponse">
		<map-to xml="keha" ns-prefix="xtee" />
		<field name="success" type="ee.adit.pojo.Success" >
		</field>
		<field name="messages" type="ee.adit.pojo.ArrayOfMessage">
			<bind-xml name="messages" />
		</field>
	</class>	

	<!-- UnJoin -->
	<class name="ee.adit.pojo.UnJoinRequest">
		<map-to xml="UnJoin" ns-uri="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid" />
		<field name="keha" type="java.lang.String">
			<bind-xml name="keha" />
		</field>
	</class>

	<class name="ee.adit.pojo.UnJoinResponse">
		<map-to xml="keha" ns-prefix="xtee" />
		<field name="success" type="ee.adit.pojo.Success" >
		</field>
		<field name="messages" type="ee.adit.pojo.ArrayOfMessage">
			<bind-xml name="messages" />
		</field>
	</class>

	<!-- GetJoined -->
	<class name="ee.adit.pojo.GetJoinedRequest">
		<map-to xml="GetJoined" ns-uri="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid" />
		<field name="maxResults" type="java.math.BigInteger">
			<bind-xml name="maxResults" location="keha" />
		</field>
		<field name="startIndex" type="java.math.BigInteger">
			<bind-xml name="startIndex" location="keha" />
		</field>
	</class>

	<class name="ee.adit.pojo.GetJoinedResponse">
		<map-to xml="keha"/>
		<field name="success" type="ee.adit.pojo.Success" >
		</field>
		<field name="messages" type="ee.adit.pojo.ArrayOfMessage">
			<bind-xml name="messages" />
		</field>
		<field name="userList" type="ee.adit.pojo.UserList">
			<bind-xml name="user_list" />
		</field>
	</class>

	<class name="ee.adit.pojo.GetJoinedResponseAttachment">
		<map-to xml="user_list" />
		<field name="total" type="java.lang.Integer">
			<bind-xml name="total" />
		</field>
		<field name="users" type="ee.adit.pojo.GetJoinedResponseAttachmentUser" collection="arraylist">
			<bind-xml name="user" />
		</field>
	</class>
 	
 	<class name="ee.adit.pojo.GetJoinedResponseAttachmentUser">
 		<map-to xml="user" />
 		<field name="code" type="java.lang.String">
			<bind-xml name="code" />
		</field>
		<field name="name" type="java.lang.String">
			<bind-xml name="name" />
		</field>
		<field name="type" type="java.lang.String">
			<bind-xml name="type" />
		</field>
 	</class>
 	
 	<!-- GetUserInfo -->
 	<class name="ee.adit.pojo.GetUserInfoRequest">
		<map-to xml="GetUserInfo" ns-uri="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid" />
		<field name="userList" type="bytes">
			<bind-xml name="userList" location="keha" />
		</field>
	</class>
 	
 	<class name="ee.adit.pojo.GetUserInfoRequestAttachmentUserList">
		<map-to xml="getUserInfoRequestAttachmentV1" />
		<field name="codes" type="java.lang.String" collection="arraylist">
			<bind-xml name="code" location="user_list"/>
		</field>
	</class>
	
 	<class name="ee.adit.pojo.GetUserInfoResponse">
		<map-to xml="keha"/>
		<field name="success" type="ee.adit.pojo.Success" >
		</field>
		<field name="messages" type="ee.adit.pojo.ArrayOfMessage">
			<bind-xml name="messages" />
		</field>
		<field name="userList" type="ee.adit.pojo.UserList">
			<bind-xml name="user_list" />
		</field>
	</class>
 	
 	<class name="ee.adit.pojo.GetUserInfoResponseAttachment">
 		<map-to xml="getUserInfoResponseAttachmentV1" />
 		<field name="userList" type="ee.adit.pojo.GetUserInfoResponseAttachmentUser" collection="arraylist">
			<bind-xml name="user_list" />
		</field>
 	</class>
 	
 	<class name="ee.adit.pojo.GetUserInfoResponseAttachmentUser">
 		<map-to xml="user" />
 		<field name="userCode" type="java.lang.String">
			<bind-xml name="code" />
		</field>
		<field name="hasJoined" type="boolean">
			<bind-xml name="has_joined" />
		</field>
		<field name="freeSpace" type="java.lang.Long">
			<bind-xml name="free_space_bytes" />
		</field>
		<field name="usedSpace" type="java.lang.Long">
			<bind-xml name="used_space_bytes" />
		</field>
		<field name="canRead" type="boolean">
			<bind-xml name="can_read" />
		</field>
		<field name="canWrite" type="boolean">
			<bind-xml name="can_write" />
		</field>
		<field name="usesDVK" type="boolean">
			<bind-xml name="uses_dvk" />
		</field>
 	</class>
 	
 	<!-- SaveDocument -->
 	<class name="ee.adit.pojo.SaveDocumentRequest">
		<map-to xml="SaveDocument" ns-uri="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid" />
		<field name="document" type="ee.adit.pojo.SaveDocumentRequest.Document">
			<bind-xml name="document" location="keha" />
		</field>
	</class>
	
	<class name="ee.adit.pojo.SaveDocumentRequest.Document">
		<map-to xml="document" ns-uri="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid" />
		<field name="href" type="java.lang.String">
			<bind-xml name="href" node="attribute" />
		</field>
	</class>
	
	<class name="ee.adit.pojo.SaveDocumentResponse">
		<map-to xml="SaveDocumentResponse" ns-uri="http://producers.ametlikud-dokumendid.xtee.riik.ee/producer/ametlikud-dokumendid" />
		<field name="success" type="ee.adit.pojo.Success" >
		</field>
		<field name="messages" type="ee.adit.pojo.ArrayOfMessage">
			<bind-xml name="messages" />
		</field>
		<field name="documentId" type="java.lang.Integer">
			<bind-xml name="document_id" />
		</field>
	</class>
 	
 	<!-- Common -->
 	<class name="ee.adit.pojo.Success">
		<map-to xml="success" ns-prefix="adit" />
		<field name="success" type="java.lang.Boolean" container="true">
		</field>
	</class>

	<class name="ee.adit.pojo.ArrayOfMessage">
		<map-to xml="messages" ns-prefix="adit" />
		<field name="message" type="java.lang.String" collection="arraylist">
			<bind-xml name="message" />
		</field>
	</class>
 	
 	<class name="ee.adit.pojo.UserList">
		<map-to xml="user_list" ns-prefix="adit"/>
		<field name="href" type="java.lang.String">
			<bind-xml name="href" node="attribute"/>
		</field>
	</class>
 	
</mapping>